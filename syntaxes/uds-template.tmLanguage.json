{
  "scopeName": "source.uds-template",
  "patterns": [
    {
      "match": "^\\s*#>#.*$",
      "name": "comment.line.logheader.uds-template"
    },
    {
      "match": "^\\s*#>.*$",
      "name": "comment.line.log.uds-template"
    },
    {
      "match": "^\\s*#.*$",
      "name": "comment.line.hidden.uds-template"
    },
    {
      "match": ";.*$",
      "name": "comment.line.inline.uds-template"
    },
    {
      "match": "!\\b(?:prog|set|baud|activate|canalyzer|canid|sa)\\b\\s*\\([^)]*\\)",
      "name": "support.function.canape.uds-template"
    },
    {
      "match": "\\@\\([^)]+\\)",
      "name": "variable.parameter.ini.uds-template"
    },
    {
      "match": "\\$EXPECTED\\([^)]*\\)",
      "name": "variable.parameter.expected.uds-template"
    },
    {
      "match": "\\$\\([^)]+\\)",
      "name": "variable.parameter.postprocess.uds-template"
    },
    {
      "match": "\\$\\@\\([^)]+\\)",
      "name": "variable.parameter.postprocessini.uds-template"
    },
    {
      "match": "\\b(?:27|10|11|34|36|31|22|2E|23|24|14|19|3E)(?:,\\s*[0-9A-Fa-f]{2})*\\b",
      "name": "constant.numeric.uds-service.uds-template"
    },
    {
      "match": "^\\s*\\[.*?\\]",
      "name": "keyword.control.section.uds-template"
    },
    {
      "match": "^\\s*[^=\\s]+(?=\\s*=)",
      "name": "variable.parameter.key.uds-template"
    },
    {
      "match": "=\\s*.*$",
      "name": "string.quoted.value.uds-template"
    }
  ]
}
