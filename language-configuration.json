{
    "comments": {
      "lineComment": "#"
    },
    "folding": {
    "markers": {
      "start": "^\\s*@IF\\b",
      "end": "^\\s*@ENDIF\\b"
        }
    },
    "brackets": [
      ["[", "]"],
      ["{", "}"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      { "open": "[", "close": "]" },
      { "open": "{", "close": "}" },
      { "open": "(", "close": ")" },
      { "open": "\"", "close": "\"" }
    ]
}
